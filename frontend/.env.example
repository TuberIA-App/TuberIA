# ============================================
# TuberIA Frontend - Environment Configuration
# ============================================
# Copy this file to .env and fill in your values
# WARNING: These values are exposed to the browser!
# Do NOT put secrets or sensitive data here!
# ============================================

# API URL - Backend endpoint
VITE_API_URL=http://localhost:5000/api

# Application name
VITE_APP_NAME=TuberIA

# ============================================
# Observability (Optional)
# ============================================
# Sentry DSN for error tracking (frontend)
# Get yours at: https://sentry.io/
# Leave empty to disable Sentry
VITE_SENTRY_DSN=
